import java.util.ArrayList;

//import java
public class Cv {
	
	String nom, prenom, formation, attentes;
	String[] competences;
	int experience;
	//ArrayList<Object> input;
	
	public static void main( String[] args ) {
		// TODO Auto-generated method stub
		System.out.println( "Bienvenue chez Barette!" );
	}
	
	
	public Cv (String nomIn, String prenomIn, String formationIn, int expIn, String[] compIn, String attentesIn) {
		this.nom = nomIn;
		this.prenom = prenomIn;
		this.formation = formationIn;
		this.experience = expIn;
		this.competences = compIn;
		this.attentes = attentesIn;
	}
	
	/*public Cv (ArrayList<Object> input) {
		
	}*/
	
	public void affiche () {
		System.out.println(this.nom + "\n");
		System.out.println(this.prenom + "\n");
		System.out.println(this.formation + "\n");
		System.out.println(this.experience + "\n");
		System.out.println(this.competences + "\n");
		System.out.println(this.attentes + "\n");
		System.out.println( "-------------------------------------------------" );
	}
}
